  "description": "A demo of popular npm packages for CLI development.",
  "type": "module",
  "bin": {
    "try": "bin/try.js"
  },
  "scripts": {
    "compile": "tsc",
    "watch-compile": "nodemon --ext ts --watch \"**\" --exec \"npm run compile || exit 1\""
  },
