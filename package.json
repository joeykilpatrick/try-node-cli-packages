    "emphasize": "^6.0.0",
    "meow": "^11.0.0",
    "minimist": "^1.2.7",
    "mri": "^1.2.0",
    "nopt": "^7.0.0",
    "sade": "^1.8.1",
    "yargs": "^17.6.2",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/nopt": "^3.0.29",
    "@types/yargs": "^17.0.20",
